{
  "name": "yekacouchacademy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "lint": "next lint",
    "server": "set PORT=3001 && node server/server.js",
    "dev:full": "concurrently \"set PORT=3000 && next dev\" \"set PORT=3001 && node server/server.js\"",
    "prod": "set \"NODE_ENV=production\" && set PORT=3001 && set NEXT_DISABLE_COMPILATION=true && node server/server.js",
    "seed": "node server/utils/seedDatabase.js",
    "reset": "node server/utils/resetDatabase.js",
    "migrate": "cd server && npx sequelize-cli db:migrate",
    "migrate:undo": "cd server && npx sequelize-cli db:migrate:undo",
    "migrate:status": "cd server && npx sequelize-cli db:migrate:status",
    "migrate:production": "cd server && cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "deploy": "npm run build && npm run migrate:production && npm run prod"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "@mui/x-date-pickers": "^6.20.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^11.0.8",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "next": "^15.3.1",
    "node-cron": "^3.0.3",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-big-calendar": "^1.18.0",
    "react-calendar": "^4.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0",
    "sequelize": "^6.37.6",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1"
  }
}
